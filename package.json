{
  "name": "my-demos",
  "version": "1.0.0",
  "description": "my demos",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "rm:nek-ui": "shx rm -rf ./node_modules/nek-ui",
    "rm:dist": "shx rm -rf ./nek-ui/dist",
    "cp:dist": "shx cp -r ./node_modules/nek-ui/dist ./nek-ui/",
    "update": "npm run rm:nek-ui && npm install && npm run rm:dist && npm run cp:dist",
    "rm:src": "shx rm -rf ./nek-ui/src",
    "cp:src": "shx cp -r ../../d/nek-ui/src ./nek-ui/",
    "update:src": "npm run rm:src && npm run cp:src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smallcosmos/demos.git"
  },
